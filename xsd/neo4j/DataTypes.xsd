<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="node">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="labels" type="xsd:anyURI" />
				<xsd:element name="self" type="xsd:anyURI" />
				<xsd:element ref="data" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="data">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="title" type="xsd:string"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
	<!-- Added also my types -->
	
	<xsd:complexType name="MyNode">
		<xsd:sequence>
			<xsd:element name="id" type="xsd:int" minOccurs="1" maxOccurs="1" />
			<xsd:element name="data" type="MyNodeBody" minOccurs="1" maxOccurs="1" />	
		</xsd:sequence>
		<xsd:attribute name="title" type="xsd:string" use="required"/>
		<xsd:attribute name="subtitle" type="xsd:string" use="required"/>
		<xsd:attribute name="code" type="xsd:string" use="required"/>
	</xsd:complexType>
	
	<xsd:complexType name="MyNodeBody">
		<xsd:sequence>
		<xsd:element name="self" type="xsd:anyURI" minOccurs="1" maxOccurs="1" />
			<!-- <xsd:element name="metadata">
				<xsd:complexType>
						<xsd:sequence>
						 	<xsd:element name="id" type="xsd:nonNegativeInteger"></xsd:element>
						 	<xsd:element name="labels" type="xsd:anyURI" minOccurs="0" maxOccurs="unbounded" />
						</xsd:sequence>
					</xsd:complexType>
			</xsd:element> -->
			<xsd:element name="data">
				<xsd:complexType>
					<xsd:sequence>

					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>	
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:element name="MyRelationship"> 
	<xsd:complexType>
		<xsd:sequence>
			<xsd:element name="type" type="xsd:string" minOccurs="1" maxOccurs="1" />
			<xsd:element name="to" type="xsd:anyURI" minOccurs="1" maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	</xsd:element>
	
	<xsd:complexType name="MyRelationshipBody">
		<xsd:sequence>
			<xsd:element name="self" type="xsd:anyURI" />
			<xsd:element name="start" type="xsd:anyURI" />
			<xsd:element name="end" type="xsd:anyURI" />
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="MyPathList">
      <xsd:sequence>
        <xsd:element name="el" type="MyPath" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="MyPath">
      <xsd:sequence>
        <xsd:element name="self" type="xsd:anyURI" minOccurs="1" maxOccurs="1"/>
        <xsd:element name="data" minOccurs="1" maxOccurs="1">
					<xsd:complexType>
						<xsd:attribute name="title" type="xsd:string" use="required" />
						<xsd:attribute name="subtitle" type="xsd:string" use="required" />
					</xsd:complexType>
				</xsd:element>
      </xsd:sequence>
    </xsd:complexType>
	  
	<xsd:complexType name="MyPathReq">
		<xsd:sequence>
			<xsd:element name="relationships">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="type" type="xsd:string" minOccurs="1" maxOccurs="1"/>
						<xsd:element name="direction" type="xsd:string" minOccurs="1" maxOccurs="1"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="max_depth" type="xsd:int" use="required"></xsd:attribute>
	</xsd:complexType>
	
	
</xsd:schema>